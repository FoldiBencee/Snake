<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/hatter"
    tools:context=".fomenu4Activity">


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/idbttnJatekInditas"
        android:background="#FFCC65"
        android:text="jatek inditasa"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/idbttnBolt"
        android:background="#FFCC65"
        android:text="bolt"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/idbttnKilepes"
        android:background="#FFCC65"
        android:text="kilepes"
        />



    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/idbttnKilelentkezes"
        android:background="#FFCC65"
        android:text="Kijelentkezes"
        />


</LinearLayout>